    "# 九九乘法表\n",
    "\n",
    "# row = 0      行\n",
    "# column = 0   列\n",
    "\n",
    "for first in range(1,10):\n",
    "    for second in  range(1,first+1):\n",
    "        print(first,\"*\",second,\"=\",first * second,end = \"\\t\")\n",
    "    print()"
